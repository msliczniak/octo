:alias y vd
:alias x ve

: main
hires
i := dashes

x := 1
loop
	y := 120
	loop
		sprite y x 3

		while y != 0
		
		y -= 8
	again
	
	while x != 57
	
	x += 7	
again

y := 8
x := 2
i := hcap
sprite y x 8

: foreever
i := hchm

loop
	vf := key
	sprite y x 15
	
	while x != 58

	x += 7
again
	
x := 2
i := hcap
sprite y x 8
jump foreever

: dashes 0x88 0x88 0x88

: hcap 0x1C 0x1C 0x14 0x08 0x08 0x14 0x1C 0x1C

: hchm 0x1C 0x1C 0x14 0x08 0x08 0x14 0x1C 0x00 0x1C 0x14 0x08 0x08 0x14 0x1C 0x1C
